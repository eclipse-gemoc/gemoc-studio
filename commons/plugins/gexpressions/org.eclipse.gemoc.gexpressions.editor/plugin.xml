<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated gexpressions -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="org.eclipse.gemoc.gexpressions.presentation.GexpressionsModelWizardID"
            name="%_UI_GexpressionsModelWizard_label"
            class="org.eclipse.gemoc.gexpressions.presentation.GexpressionsModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/GexpressionsModelFile.gif">
         <description>%_UI_GexpressionsModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated gexpressions -->
      <editor
            id="org.eclipse.gemoc.gexpressions.presentation.GexpressionsEditorID"
            name="%_UI_GexpressionsEditor_label"
            icon="icons/full/obj16/GexpressionsModelFile.gif"
            extensions="gexpressions"
            class="org.eclipse.gemoc.gexpressions.presentation.GexpressionsEditor"
            contributorClass="org.eclipse.gemoc.gexpressions.presentation.GexpressionsActionBarContributor">
      </editor>
   </extension>
</plugin>
