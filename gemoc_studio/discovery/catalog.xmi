<?xml version="1.0" encoding="ASCII"?>
<discovery:DiscoveryDefinition
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:discovery="http://www.eclipse.org/amalgamation/discovery/1.0">
  <categories
      name="GEMOC Language"
      description="Languages designed using GEMOC."
      relevance="7">
    <components
        name="ActivityDiagram Languages (Sequential/Concurrent)(Incubation)"
        provider="GEMOC"
        description="Deployer that installs the project sources for Concurrent and/or Sequential version of the Activitydiagram Language and sample models."
        license="EPL"
        groups="//@filters.0"
        image32="images/IconeGemocLanguagePlusModel-32.png">
      <sitesURLS>https://ci.inria.fr/gemoc/job/ActivityDiagram_updatesite/lastSuccessfulBuild/artifact/dev/releng/org.gemoc.activitydiagram.repository/target/repository/</sitesURLS>
      <overview
          summary="Deployer that installs the project sources for two versions of the Activitydiagram Language developped using GEMOC. One version will run using the GEMOC Sequential engine, the second using Concurrent engine.  The deployer also provides sample models."
          screenshot="images/activitydiagram_sequential-screenshot-320x240.png"
          url="http://github.com/gemoc/activitydiagram"/>
      <id>org.gemoc.activitydiagram.feature</id>
      <messages
          title="Getting started"
          message="Once Eclipse has restarted, your example languages will be available in the File > New > Example... menu. Look for the GEMOC categories. Some more informations will be available in the Eclipse help."/>
    </components>
    <components
        name="ArduinoModeling Languages (Sequential/Concurrent)(Incubation)"
        provider="GEMOC"
        description="Deployer that installs the project sources for Sequential and Concurrent  versions of the ArduinoModeling Language and sample models."
        license="EPL"
        groups="//@filters.0"
        image32="images/IconeGemocLanguagePlusModel-32.png">
      <sitesURLS>https://ci.inria.fr/gemoc/job/ArduinoModeling_updatesite/lastSuccessfulBuild/artifact/dev/releng/org.gemoc.arduinomodeling.repository/target/repository/</sitesURLS>
      <overview
          summary="Deployer that installs the project sources for two versions of the ArduinoModeling Language developped using GEMOC.  One version run using the GEMOC Sequential engine, the second using the Concurrent engine. The deployer also provides some sample models."
          screenshot="images/arduinomodeling_sequential-screenshot-320x240.png"
          url="http://github.com/gemoc/arduinomodeling"/>
      <id>org.gemoc.arduinomodeling.feature</id>
      <messages
          title="Getting started"
          message="Once Eclipse has restarted, your example languages will be available in the File > New > Example... menu. Look for the GEMOC categories. Some more informations will be available in the Eclipse help."/>
    </components>
    <components
        name="Markedgraph Language (Concurrent)(Incubation)"
        provider="GEMOC"
        description="Deployer that installs the project sources for the concurrent version of the Markedgraph Language and sample models."
        license="EPL"
        groups="//@filters.0"
        image32="images/IconeGemocLanguagePlusModel-32.png">
      <sitesURLS>https://ci.inria.fr/gemoc/job/Markedgraph_updatesite/lastSuccessfulBuild/artifact/sample/markedgraph/releng/org.gemoc.sample.markedgraph.repository/target/repository/</sitesURLS>
      <overview
          summary="Deployer that installs the project sources for the Markedgraph Language developped using GEMOC.  This version runs using the GEMOC Concurrent engine. The deployer also provides some sample models. This is the result of the GEMOC tutorial available in the eclipse help."
          screenshot="images/markedgraph_sequential-screenshot-320x240.png"
          url="http://gemoc.github.io/gemoc-studio/publish/tutorial_markedgraph/html_single/GuideTutorialMarkedGraph.html"/>
      <id>org.gemoc.sample.markedgraph.feature</id>
      <messages
          title="Getting started"
          message="Once Eclipse has restarted, your example languages will be available in the File > New > Example... menu. Look for the GEMOC categories. Some more informations will be available in the Eclipse help."/>
    </components>
  </categories>
  <categories
      name="GEMOC Coordination"
      description="Language coordination operators designed using GEMOC."
      relevance="6">
    <components
        name="Operator for TFSM and ActivityDiagram (Incubation)"
        provider="I3S"
        description="Deployer that installs the project sources for the coordination operator for TFSM and ActivityDiagram concurrent languages and sample models."
        license="EPL"
        groups="//@filters.0"
        image32="images/bcool-icon-32.png">
      <sitesURLS>https://ci.inria.fr/gemoc/job/gemoc-studio_official_samples_deployer/lastSuccessfulBuild/artifact/official_samples/sample.deployers/releng/org.eclipse.gemoc.samples.deployers.repository/target/repository/</sitesURLS>
      <overview
          summary="Deployer that installs the project sources for the coordination operator for TFSM and ActivityDiagram concurrent languages developped using GEMOC. The deployer also provides some sample models."
          screenshot="images/bcool-tfsm-activity-320x240.png"
          url="http://timesquare.inria.fr/BCOoL/"/>
      <id>org.eclipse.gemoc.coordination.samples.deployers.feature</id>
      <messages
          title="Getting started"
          message="Once Eclipse has restarted, your example languages will be available in the File > New > Example... menu. Look for the GEMOC categories. Some more informations will be available in the Eclipse help."/>
    </components>
  </categories>
  <categories
      name="Alternative GEMOC based Engines"
      description="Engines and tools proposing alternative approaches (based on GEMOC execution framework)">
    <components
        name="GEMOC xMOF Engine"
        provider="Vienna University of Technology"
        description="xMOF allows you to define the execution semantics of your language with UML activity diagrams."
        license="EPL"
        image32="images/xmof-icon-30x30.png">
      <sitesURLS>http://moliz.github.io/moliz.gemoc/updatesite/nightly/</sitesURLS>
      <sitesURLS>http://download.eclipse.org/releases/neon</sitesURLS>
      <overview
          summary="The xMOF language allows you to define the execution semantics of your language with UML activity diagrams. This component provides the xMOF language, editor and execution engine, as well as wizards helping you to setup your xMOF projects."
          screenshot="images/xmof-figure-320x240.png"
          url="https://github.com/moliz/moliz.xmof"/>
      <id>org.modelexecution.moliz.gemoc.feature</id>
    </components>
  </categories>
  <categories
      name="Exhaustive Exploration"
      description="Tools for Exhaustive Exploration."
      relevance="4">
    <components
        name="Clocksystem (Incubation)"
        provider="ENSTA"
        description="Multiform Time Constraints"
        license="MIT"
        groups="//@filters.0"
        image32="images/clocksystem_logo_32x32.png">
      <sitesURLS>https://ci.inria.fr/gemoc/job/concurrency_exploration/lastSuccessfulBuild/artifact/exploration/releng/org.gemoc.mocc.clocksystem.features.updatesite/target/repository</sitesURLS>
      <overview
          summary="Clocksystem is meta-described clock-constraint engine, which embeds a formal model of logical time into the Smalltalk general-purpose language and environment"
          screenshot="images/cs_screenshot_320x240.png"/>
      <id>org.gemoc.mocc.clocksystem.common.feature</id>
    </components>
  </categories>
  <categories
      name="Modeler"
      description="Modeling environment tools."
      relevance="3">
    <components
        name="UML Designer"
        provider="Obeo"
        description="Graphical tooling to edit and visualize UML models."
        license="EPL"
        image32="images/umldesigner.png">
      <sitesURLS>http://download.eclipse.org/releases/luna/201502271000</sitesURLS>
      <sitesURLS>http://download.eclipse.org/sirius/updates/nightly/3.0.x/luna</sitesURLS>
      <sitesURLS>http://releases.umldesigner.org/5.0.1/repository/</sitesURLS>
      <overview
          summary="UML Designer provides a set of common diagrams to work with UML 2.5 models. The intent is to provide an easy way to make the transition from UML to domain specific modeling. This way users can continue to manipulate legacy UML models and start working with DSL. Users can even re-use the provided representations and work in a total transparence on both UML and DSL models at the same time."
          screenshot="images/umldesigner-screenshot-320x240.png"
          url="http://www.umldesigner.org/"/>
      <id>org.obeonetwork.dsl.uml2.runtime.feature</id>
    </components>
  </categories>
  <categories
      name="Alternative Tools"
      description="Engines and tools proposing alternative approaches (may not directly use GEMOC execution framework)"
      relevance="2">
    <components
        name="ALE (Incubation)"
        provider="Inria and Obeo"
        description="Action Language for EMF"
        license="EPL"
        image32="images/modeling32.png">
      <sitesURLS>https://ci.inria.fr/gemoc/job/ale-lang/lastSuccessfulBuild/artifact/releng/org.eclipse.emf.ecoretools.ale.updatesite/target/repository/</sitesURLS>
      <overview
          summary="Ale is a language to define behaviors on the top of Ecore models. It is intended to be distributed in EcoreTools and will be an alternative to K3."
          url="http://gemoc.org/ale-lang/"/>
      <id>org.eclipse.emf.ecoretools.ale.feature</id>
    </components>
    <components
        name="TESL Engine and Editor"
        provider="CentraleSup&#xe9;lec/LRI"
        description="The Tagged Event Specification Language allows the specification of timed systems by modeling events as clocks and occurrences of an event as ticks on its clock."
        license="Unknown Licence"
        groups="//@filters.0"
        incubation="true">
      <sitesURLS>http://wwwdi.supelec.fr/tesl-update-site</sitesURLS>
      <overview
          summary="The Tagged Event Specification Language allows the specification of timed systems by modeling events as clocks and occurrences of an event as ticks on its clock."
          screenshot="images/tesl-screenshot-320x240.png"
          url="http://wwwdi.supelec.fr/software/TESL/"/>
      <id>fr.supelec.tesl.feature</id>
    </components>
  </categories>
  <filters name="Incubation"/>
</discovery:DiscoveryDefinition>
